# .env.template

# Stripe secret key (keep the real value in your .env, never commit it)
STRIPE_SECRET_KEY=your_stripe_secret_key_here

# Webhook secret if you're using Stripe webhooks
STRIPE_WEBHOOK_SECRET=your_webhook_secret_here

# Optional: SendGrid or other service key
SENDGRID_API_KEY=your_sendgrid_api_key_here

# Server port
PORT=4242
